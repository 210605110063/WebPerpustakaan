@model WebPerpustakaan.Models.MasterKategoriControllerVM.Index


@{
    
}

<table border="1" cellpadding="5" cellspacing="0" style="border-collapse: collapse; width: 100%;">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nama</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.DataKategori)
        {
            <tr>
                <td>@item.IdKategori</td>
                <td>@item.NamaKategori</td>
            </tr>
        }
    </tbody>
</table>
